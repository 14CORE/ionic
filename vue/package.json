    "rollup-plugin-typescript": "1.0.0",
    "rollup-plugin-vue": "^4.3.0",
    "tslint": "5.11.0",
    "tslint-ionic-rules": "0.0.19",
    "typescript": "3.1.3",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "dependencies": {
    "@ionic/core": "4.0.0-beta.12"
